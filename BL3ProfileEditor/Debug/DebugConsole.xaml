<Window x:Class="BL3ProfileEditor.Debug.DebugConsole"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BL3ProfileEditor.Debug"
        mc:Ignorable="d"
        Title="Debug Console" Height="450" Width="450" WindowStyle="ToolWindow">
    <Grid>
        <TextBox Name="textBoxDebug" ScrollViewer.VerticalScrollBarVisibility="Visible" TextWrapping="Wrap" IsReadOnly="True" IsReadOnlyCaretVisible="True"/>
    </Grid>
</Window>
